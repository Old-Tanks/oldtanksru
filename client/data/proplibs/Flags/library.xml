<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<library name="Flags">
    <prop-group name="default">
        <prop name="FlagBLUE">
            <mesh file="object.3ds">
			<texture name="Blue" diffuse-map="blue.jpg"/>

            </mesh>
        </prop>
        <prop name="Flag">
            <mesh file="object.3ds">
			<texture name="Blue" diffuse-map="blue.jpg"/>

			<texture name="Red" diffuse-map="red.jpg"/>

            </mesh>
        </prop>
        <prop name="FlagRED">
            <mesh file="object.3ds">
			<texture name="Red" diffuse-map="red.jpg"/>		
            </mesh>
        </prop>
		<prop name="Flag Sprite Blue">
		<sprite file="blue_flag_ny.gif" origin-y="0.99" scale="1.4"/>

		
		</prop>
		<prop name="Flag Sprite Red">
		<sprite file="red_flag_ny.gif" origin-y="0.99" scale="1.4"/>
		
		</prop>
    </prop-group>
</library>
